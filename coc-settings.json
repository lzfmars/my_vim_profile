{
  "languageserver": {
  	"golang": {
  	  "command": "~/go/bin/gopls",
  	  "filetypes": ["go"],
  	  "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
  	  "initializationOptions": {
		"usePlaceholders": true
  	  }
	}
  },

  "python.linting.enabled": false,
  "python.pythonPath": "python3",
  "diagnostic.enable": false,
}
